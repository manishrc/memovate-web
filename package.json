{
  "name": "memovate-web",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "dev:all": "npm-run-all --parallel dev db:studio email",
    "build": "next build",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "next start",
    "lint": "next lint",
    "email": "email dev --port 3002",
    "db:migrate": "drizzle-kit generate",
    "db:pull": "drizzle-kit introspect",
    "db:push": "drizzle-kit push",
    "db:drop": "drizzle-kit drop",
    "db:update": "drizzle-kit up",
    "db:check": "drizzle-kit check",
    "db:studio": "drizzle-kit studio --port 3001",
    "add:component": "npx shadcn-ui@latest add",
    "update": "npx npm-check -u"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.1.0",
    "@headlessui/react": "^2.1.0",
    "@neondatabase/serverless": "^0.9.3",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@react-email/components": "^0.0.19",
    "@react-email/render": "^0.0.15",
    "@sentry/nextjs": "^7.114.0",
    "@tailwindcss/typography": "^0.5.13",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cva": "npm:class-variance-authority@^0.7.0",
    "drizzle-orm": "^0.30.10",
    "framer-motion": "^11.2.11",
    "lucide-react": "^0.379.0",
    "nanoid": "^5.0.7",
    "nanoid-dictionary": "^4.3.0",
    "next-auth": "^5.0.0-beta.17",
    "next-themes": "^0.3.0",
    "psl": "^1.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-email": "^2.1.4",
    "react-icons": "^5.2.1",
    "resend": "3.2.0",
    "server-only": "^0.0.1",
    "sonner": "^1.4.41",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.3",
    "@tailwindcss/forms": "^0.5.7",
    "autoprefixer": "^10.4.18",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.21.4",
    "eslint": "^8.2.0",
    "eslint-config-next": "^14.2.3",
    "next": "^14.2.3",
    "nodemailer": "^6.9.12",
    "npm-run-all": "^4.1.5",
    "pg": "^8.11.3",
    "postcss": "^8.4.35",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7"
  }
}
